{
  "name": "townly",
  "version": "0.1.0",
  "description": "",
  "main": "dist/Townly.js",
  "scripts": {
    "start": "npm run compile && electron --trace-warnings .",
    "watch": "concurrently \"tsc -w\" \"electron --trace-warnings .\"",
    "compile": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sv443/Townly.git"
  },
  "author": {
    "name": "Sv443",
    "url": "https://github.com/Sv443",
    "email": "contact@sv443.net"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sv443/Townly/issues"
  },
  "homepage": "https://github.com/Sv443/Townly#readme",
  "build": {
    "appId": "com.sv443",
    "files": [
      "src/index.js",
      "assets/*"
    ],
    "mac": {
      "target": "dmg",
      "icon": "assets/icon_1000.png"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon_1000.png"
    },
    "linux": {
      "target": "deb",
      "icon": "assets/icon_1000.png",
      "category": "Development"
    }
  },
  "dependencies": {
    "excalibur": "^0.25.1",
    "fs-extra": "^10.0.0",
    "svcorelib": "^1.14.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "concurrently": "^6.5.1",
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5",
    "eslint": "^8.5.0",
    "typescript": "^4.5.4"
  }
}

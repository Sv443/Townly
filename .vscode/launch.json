    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\Townly.js",
            "console": "integratedTerminal"
        },
        {
            "program": "${workspaceFolder}/test.js",
            "name": "Test.js",
            "request": "launch",
            "type": "node",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "--trace-deprecation"
            ],
            "console": "integratedTerminal"
        }
    ]
}